<header class="bg-gradient-to-b from-custom-500 to-sky-700" x-data="{ mobileMenu : false }">
  <nav class="mx-auto flex items-center max-w-7xl justify-between p-6 lg:px-8" aria-label="Global"  x-on:click=" mobileMenu = ! mobileMenu ">
    <div class="flex flex-1">
      <div class="hidden w-full lg:flex lg:gap-x-12 justify-between"
        x-data="{
        new2flying : false,
        trainProgram : false,
        about : false }"
      >
        <%# Links %>
        <button class="text-base xl:text-2xl tracking-wider uppercase text-white hover:scale-125 duration-300" x-on:click="new2flying = ! new2flying, trainProgram = false, about = false">New to <br>
          Flying?</button>
        <button class="text-base xl:text-2xl tracking-wider uppercase text-white hover:scale-125 duration-300" x-on:click="trainProgram = ! trainProgram, new2flying = false, about = false">Training <br>
          Programs</button>
        <button class="text-base xl:text-2xl tracking-wider uppercase text-white hover:scale-125 duration-300" x-on:click="about = ! about, new2flying = false, trainProgram = false">About <br>
          Us</button>
